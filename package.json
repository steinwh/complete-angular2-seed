  "version": "1.0.0",
  "description": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "start": "webpack-dev-server --env.TARGET DEV",
    "postinstall": "typings i",
    "lint": "tslint src/**/*ts",
