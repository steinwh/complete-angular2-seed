    "ts-node": "^0.5.5",
    "tslint": "^3.4.0",
    "typescript": "^1.8.2",
    "typings": "^0.6.8",
    "watch": "^0.17.1",
    "webpack": "^2.1.0-beta.4",
    "webpack-dev-server": "^1.14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomastrajan/complete-angular2-seed.git"
  },
  "keywords": [
